some_struct: struct {
	random_field: int;
};

example<T>: () -> void {
	
	a: T;			/* a's type is undetermined, don't typecheck any expression with it */
	b: some_struct; /* b's type is determined, typecheck normally */

	x: int;
	y: int;
	
	x = (a + 13) * b;	/* isn't typechecked */
	y = (b + 13) * 17;  /* is typechecked, and an error is reported */
};

factorial: (n: int) -> int {
	if (n <= 1)
		return 1;
	return n * factorial(n - 1);
}
